{
    "name": "edjw/breeze-lite",
    "version": "0.0.1",
    "description": "Temporary fork from senkulabs/breeze-lite Unofficial Laravel Breeze for Laravel + Inertia + Svelte.",
    "keywords": [
        "laravel-breeze",
        "laravel",
        "inertia",
        "svelte"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "illuminate/console": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "symfony/console": "^7.0",
        "laravel/breeze": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "edjw\\Breeze\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "edjw\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}